{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3CredentialsAccess",
            "Effect": "Deny",
            "Action": [
                "s3:DeleteObject",
                "s3:PutObject",
                "s3-object-lambda:WriteGetObjectResponse"
            ],
            "Resource": "*"
        },
        {
            "Sid": "EC2CredentialsAccess",
            "Effect": "Deny",
            "Action": [
                "ec2:TerminateInstances"
            ],
            "Resource": "*"
        },
        {
            "Sid": "ElasticCacheCredentialsAccess",
            "Effect": "Deny",
            "Action": [
                "elasticache:DeleteCacheCluster",
                "elasticache:DeleteReplicationGroup"
            ],
            "Resource": "*"
        },
        {
            "Sid": "DenyDeleteAndUpdatePolicy",
            "Effect": "Deny",
            "Action": [
                "kms:ScheduleKeyDeletion",
                "kms:PutKeyPolicy"
            ],
            "Resource": "*" 
        }
    ]
}